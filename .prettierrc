{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "svelteSortOrder": "markup-scripts-styles-options",
  "svelteBracketNewLine": true,
  "svelteIndentScriptAndStyle": true,
  "svelteStrictMode": false
}
